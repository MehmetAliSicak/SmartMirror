<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="93dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv1"
        android:layout_alignStart="@+id/iv1"
        android:layout_below="@+id/iv1"
        android:layout_marginTop="25dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv2"
        android:layout_alignStart="@+id/iv2"
        android:layout_below="@+id/iv2"
        android:layout_marginTop="35dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv3"
        android:layout_alignStart="@+id/iv3"
        android:layout_below="@+id/iv3"
        android:layout_marginTop="35dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/iv4"
        android:layout_alignStart="@+id/iv4"
        android:layout_below="@+id/iv4"
        android:layout_marginTop="34dp"
        app:srcCompat="@drawable/common_google_signin_btn_icon_dark" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/iv2"
        android:layout_marginBottom="12dp"
        android:layout_toEndOf="@+id/iv1"
        android:layout_toRightOf="@+id/iv1"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/iv3"
        android:layout_marginBottom="11dp"
        android:layout_toEndOf="@+id/iv2"
        android:layout_toRightOf="@+id/iv2"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/iv3"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/iv3"
        android:layout_toRightOf="@+id/iv3"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/iv5"
        android:layout_marginBottom="11dp"
        android:layout_toEndOf="@+id/iv4"
        android:layout_toRightOf="@+id/iv4"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv5"
        android:layout_marginBottom="11dp"
        android:layout_toEndOf="@+id/iv5"
        android:layout_toRightOf="@+id/iv5"
        android:text="TextView" />
</RelativeLayout>
